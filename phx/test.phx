function int (int x) factorial:
	if x < 1:
		return 1
	return x * (x - 1)factorial

function int (int n) choose (int k):
	return (n) factorial / (k) factorial / (n-k) factorial

for int row = 0; row < 10; row += 1:
	str s = ""
	for int col = 0; col <= row; col += 1:
		s += (row) choose (col)
		s += " "
	print s